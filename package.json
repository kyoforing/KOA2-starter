{
  "name": "koa2-starter",
  "version": "0.0.0",
  "description": "koa2-starter",
  "main": "app.js",
  "author": {
    "name": "Kyo Cheng"
  },
  "scripts": {
    "start": "npm run start-prod",
    "start-dev": "better-npm-run start-dev",
    "start-stage": "better-npm-run start-stage",
    "start-pst": "better-npm-run start-pst",
    "start-prod": "better-npm-run start-prod",
    "test": "jest"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node app.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "start-prod": {
      "command": "node app.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    }
  },
  "license": "UNLICENSED ",
  "dependencies": {
    "dompurify": "^2.0.8",
    "dotenv": "^4.0.0",
    "jsdom": "^9.12.0",
    "jsonwebtoken": "^7.4.0",
    "knex": "^0.19.5",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^3.1.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.2.1",
    "koa2-cors": "^2.0.3",
    "moment": "^2.18.1",
    "mysql": "^2.13.0",
    "node-uuid": "^1.4.8",
    "request": "^2.81.0",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": "7.10.1"
  },
  "devDependencies": {
    "better-npm-run": "0.0.13",
    "ejs": "^2.5.6",
    "jest": "^26.6.0",
    "supertest": "^5.0.0"
  }
}
